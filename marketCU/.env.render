# This is a sample .env file for Render deployment
# Replace these placeholder values with actual values in the Render dashboard

# Environment
NODE_ENV=production

# Server Configuration 
PORT=3001

# Security
JWT_SECRET=REPLACE_WITH_SECURE_RANDOM_STRING

# Database
DATABASE_URL=REPLACE_WITH_RENDER_POSTGRES_URL

# CORS
CORS_ORIGIN=https://your-frontend-url.onrender.com

# Email Service (for notifications and verification)
EMAIL_USER=your-email@gmail.com
EMAIL_PASSWORD=your-email-app-password
RESEND_API_KEY=your-resend-api-key

# Admin Configuration
ADMIN_EMAILS=admin1@columbia.edu,admin2@columbia.edu

# Email Verification (set to FALSE in production)
SKIP_EMAIL_VERIFICATION=FALSE 